  "scripts": {
    "clean": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' + && yarn && yarn gatsby-clean",
    "gatsby-clean": "gatsby clean",
    "start": "npm run develop",
    "build": "NODE_ENV=production NODE_OPTIONS=--max-old-space-size=10240 cross-env node --trace-warnings node_modules/.bin/gatsby build --log-pages",
    "develop": "NODE_OPTIONS=--max-old-space-size=8192 npm run gatsby-clean && node --trace-warnings node_modules/.bin/gatsby develop --S -H 0.0.0.0",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "jest",
    "update-libs": "yarn upgrade live-event-widget openstack-uicore-foundation schedule-lite simple-chat-widget speakers-widget attendee-to-attendee-widget"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "babel-jest": "^29",
    "devcert": "1.1.0",
    "gatsby-plugin-webpack-speed-measure": "^0.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29",
    "jest-environment-jsdom": "^29",
    "jest-transform-stub": "^2.0.0",
    "js-yaml": "^4.1.0",
    "js-yaml-loader": "^1.2.2",
    "jsdom": "^24",
    "prettier": "^2.0.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "^@mdx-js/mdx$": "<rootDir>/__mocks__/@mdx-js/mdx.js",
      "^@mdx-js/react$": "<rootDir>/__mocks__/@mdx-js/react.js",
      "^remark-gfm$": "<rootDir>/__mocks__/remark-gfm.js",
      "^rehype-external-links$": "<rootDir>/__mocks__/rehype-external-links.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "globals": {
      "window": {},
      "console": {}
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  }
